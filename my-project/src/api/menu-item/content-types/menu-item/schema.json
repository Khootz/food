{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "Price": {
      "type": "decimal"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Breakfast",
        "Lunch",
        "Tea Time",
        "Snack",
        "Drinks"
      ]
    },
    "Availability": {
      "type": "boolean"
    },
    "Ingredients": {
      "type": "string"
    },
    "Size": {
      "type": "enumeration",
      "enum": [
        "Small",
        "Medium",
        "Large"
      ]
    }
  }
}
